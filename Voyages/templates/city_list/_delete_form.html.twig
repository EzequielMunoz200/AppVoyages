<form method="post" action="{{ path('city_list_delete', {'id': list.id}) }}" onsubmit="return confirm('Voulez-vous effacer cette liste?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ list.id) }}">
    <button class="btn">Effacer</button>
</form>
